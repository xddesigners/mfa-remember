{
    "name": "xddesigners/mfa-remember",
    "description": "Allow users to remember a browser so a user doesnt have to enter a code when they log in again from that browser.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "mfa",
        "2fa",
        "security",
        "authentication",
        "remember"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.4 || ^8.0",
        "silverstripe/mfa": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "XD\\MFARemember\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
